{
  "name": "wqx",
  "version": "3.0.2",
  "description": "Water Quality Exchange (WQX): JSON Schema",
  "scripts": {
    "build:values": "node --max-old-space-size=8192 bin/build-values.js",
    "build:schema": "node --max-old-space-size=8192 bin/build-schema.js",
    "build": "npm run build:values && npm run build:schema && cp -r src/deprecated dist/ && cp -r src/groups dist/ && cp -r src/required dist/ && cp -r src/values dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git@github.com/gordonfn/wqx.git"
  },
  "keywords": [
    "EPA",
    "water quality exchange",
    "WQX",
    "json-schema"
  ],
  "author": "willfarrell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gordonfn/wqx/issues"
  },
  "homepage": "https://github.com/gordonfn/wqx#readme",
  "devDependencies": {
    "change-case": "4.1.1",
    "glob": "^7.1.2",
    "json-schema-ref-parser": "^4.1.1",
    "node-fetch": "2.6.0",
    "xml-js": "^1.6.2"
  }
}
